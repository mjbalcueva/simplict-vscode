{
	"name": "simplict-vscode",
	"displayName": "SimpliCT",
	"description": "SimpliCT theme for VS Code",
	"version": "0.1.0",
	"preview": true,
	"publisher": "mjbalcueva",
	"license": "MIT",
	"galleryBanner": {
		"color": "#10161c",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mjbalcueva/simplict-theme-vscode"
	},
	"bugs": {
		"url": "https://github.com/mjbalcueva/simplict-theme-vscode/issues"
	},
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dark",
		"light",
		"simplict",
		"theme"
	],
	"contributes": {
		"themes": [
			{
				"label": "SimpliCT Dark Default",
				"uiTheme": "vs-dark",
				"path": "./themes/dark-default.json"
			},
			{
				"label": "SimpliCT Dark Dimmed",
				"uiTheme": "vs-dark",
				"path": "./themes/dark-dimmed.json"
			},
			{
				"label": "SimpliCT Light Default",
				"uiTheme": "vs",
				"path": "./themes/light-default.json"
			}
		]
	},
	"devDependencies": {
		"nodemon": "^2.0.13"
	},
	"scripts": {
		"start": "nodemon --watch src src/index.js",
		"build": "node src/index.js"
	}
}
